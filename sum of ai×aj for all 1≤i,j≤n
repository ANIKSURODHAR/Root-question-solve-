 Given an array of n integers, find the sum of ai×aj for all 1≤i,j≤n;

​#include<bits/stdc++.h>

using namespace std;

int main(){

 int n , totall_sum=0 , ans=0;

 cin>>n;

 vector<int>v(n);

 for(int i=0;i<n;i++)  cin>>v[i];

 for(int i=0;i<n;i++)  totall_sum+=v[i];

 for(int i=0;i<n;i++){

    int sum=0;

    sum=v[i]*totall_sum;

    ans+=sum;
 }

 cout<<ans<<"\n";


}


 
